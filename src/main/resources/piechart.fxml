<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="TOP_CENTER" minHeight="500.0" minWidth="800.0" prefHeight="500.0" prefWidth="800.0" style="-fx-background-color: navy;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ca.on.georgianc.PieController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="486.0" minWidth="2.0" prefWidth="2.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="592.0" minWidth="10.0" prefWidth="592.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="294.0" minWidth="10.0" prefWidth="82.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="300.0" minHeight="9.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="403.0" minHeight="10.0" prefHeight="325.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="232.0" minHeight="10.0" prefHeight="71.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Button fx:id="changeButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="96.0" style="-fx-background-color: silver;" text="Alternate View" GridPane.columnIndex="2" GridPane.rowIndex="2" onAction="#switchToTable"/>
        <PieChart fx:id="comicPie" style="-fx-background-color: #0476F2;" title="DC Comics 2021 Releases By Character" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    </children>
</GridPane>
